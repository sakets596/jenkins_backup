        <varPasswordPair var="prod_passwd" password="{AQAAABAAAAAQs6VL7myAG8eStDUhUrtXWsKZSN2GP4iZFzGHri1DCR4=}"/>
        <varPasswordPair var="dev_passwd" password="{AQAAABAAAAAQOHjZYNhtKX10UqZR9U2GGCSbuXR+y/f8FlyptbghKx8=}"/>
      </varPasswordPairs>
      <varMaskRegexes/>
    </com.michelin.cio.hudson.plugins.maskpasswords.MaskPasswordsBuildWrapper>
  </buildWrappers>
</project>